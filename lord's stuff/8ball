function getRandomInt(max) {

    return Math.floor(Math.random() * max);
  }
  
  var num = getRandomInt(25);
  console.log(num);
  // Expected output: 0-24
  
  switch (num) {
    case 0:
      res.status(200).send("It is certain");
      break;
    case 1:
      res.status(200).send("Reply hazy, try again");
      break;
